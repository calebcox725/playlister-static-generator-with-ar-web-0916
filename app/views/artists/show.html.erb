<html>
<head>
  <title><%= @artist.name %></title>
</head>
<body>
  <h1><%= @artist.name %></h1>
  <ul>
    <% @artist.genres.each do |genre| %>
      <li><a href="/genres/<%= genre.name %>.html">
        <%= genre.name %>
      </a></li>"
    <% end %>
  </ul>
  <ul>
      <% @artist.songs.each do |song| %>
      <li><a href=<%= "/songs/#{song.to_slug}.html/"%>>
        <%= "#{@artist.name} - #{song.name} - #{song.genre.name}" %>
      </a></li>"
      <% end %>
    </ul>
</body>
</html>